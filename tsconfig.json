{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "rootDirs": [
      "src"
    ],
    "outDir": "dist",
    "lib": [
      "es2021"
    ],
    "moduleResolution": "node",
    "types": [
      "node"
    ],
    "baseUrl": "."
  },
}